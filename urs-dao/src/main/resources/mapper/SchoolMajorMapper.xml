<!--MyBatis配置文件-->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nju.urs.dao.mysql.mapper.SchoolMajorMapper">

    <select id="findBySchoolIdAndMajorIdAndProvince" resultType="com.nju.urs.dao.mysql.model.po.SchoolMajor">
        select * from school_major where school_id=#{schoolId} and major_id=#{majorId} and province=#{province}
    </select>
</mapper>